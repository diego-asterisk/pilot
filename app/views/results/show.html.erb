<h1><%= t(:Result) %></h1>
<h2><%= t(:Docket_number) %> <%= @result.student.docket_number %></h2>

<p id="notice"><%= notice %></p>

<p>
  <strong><%= t(:First_name) %>:</strong>
  <%= @result.student.first_name %>
</p>

<p>
  <strong><%= t(:Last_name) %>:</strong>
  <%= @result.student.last_name %>
</p>

<p>
  <strong><%= t(:Exam_instance) %>:</strong>
  <%= @result.exam_instance.title %>
</p>

<p>
  <strong><%= t(:Score) %>:</strong>
  <%= @result.score %>
</p>



<%= link_to t(:Edit), edit_result_path(@result) %> |
<%= link_to t(:Back), results_path %>
